{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch MQTT Client",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [
                "client",
                "mqtt",
                "--mqtt_server", "tcp://192.168.1.2:1883/",
                "--mqtt_username", "phevmqttuser",
                "--mqtt_password", "mqttuserpassword"
            ],
            "env": {}
        },
        {
            "name": "Launch Client Register",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [
                "client",
                "register"
            ],
            "env": {}
        },
        {
            "name": "Docker: Attach to Container",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "/workspace",
            "port": 2345,
            "host": "localhost",
            "showLog": true
        }
    ]
}
